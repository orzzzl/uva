#include <bits/stdc++.h>
using namespace std;


int main () {
    string s;
    cin >> s;
    string x [25];
    x [10] = s;
    string tmp = s;
    int i = 9;
    while (i >= 0 && prev_permutation (tmp))
        x [i--] = tmp;
    tmp = s;
    i = 11;
    while (i <= 20 && next_permutation (tmp))
        x [i++] = tmp;

    for (int i = 0; i <= 20; i ++)
        cout << x [i] << endl;
    return 0;
}
