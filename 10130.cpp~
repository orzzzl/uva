#include <bits/stdc++.h>
using namespace std;

int T, n;
int memo [1005][35];
int price [1005];
int weight [35];

int sol (int id, int w) {
    if (w <= 0)    return 0;
    if (id == n)    return 0;
    if (memo [id][w] != -7)    return memo [id][w];
    if (weight [id] > w)    return memo [id][w] = sol (id + 1, w);
    return memo [id][w] = max (sol (id + 1, w), price [id] + sol (id + 1, w - weight [id]));
}

int main () {
    scanf ("%d", &T);
    while (T --) {
	memset (memo, -7, sizeof memo);
	memset (price, 0, sizeof prize);
	memset (weight, 0, sizeof weight);
	scanf ("%d", &n);
	for (int i = 0; i < 3; i ++)
	    scanf ("%d %d", price + i, weight + i);
	int ans = 0;
	int g = 0;
	scanf ("%d", &g);
	while (g --) {
	    int tmp;
	    scanf ("%d", &tmp);
	    ans += sol (0, tmp);
	}
	printf ("%d\n", ans);
    }
    return 0;
}
